_comsat()
{
	local cur prev opts base
	COMPREPLY=()
	cur="${COMP_WORDS[COMP_CWORD]}"
	prev="${COMP_WORDS[COMP_CWORD-1]}"
	opts="--help --version -h -? -v help version status"

	case "${prev}" in
		config)
			;;
		provision)
			;;
		*)
			;;
	esac


	COMPREPLY=($(compgen -W "${opts}" -- ${cur}))
	return 0
}
complete -F _comsat comsat